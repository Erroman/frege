--- Classification of literals.
module frege.compiler.enums.Literals where 

import frege.data.JSON (ToJSON, toJSON)

{--
 * Literals
 -}
data Literalkind =
      LBool
    | LChar
    | LString
    | LInt
    | LBig
    | LLong
    | LFloat
    | LDouble
    | LRegex


derive Enum Literalkind


derive Show Literalkind

instance ToJSON Literalkind where
  toJSON = toJSON . show
