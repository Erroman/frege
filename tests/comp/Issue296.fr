module tests.comp.Issue271 where

data RType = DeadEnd | WinningCycle | LosingCycle

exampleGood :: (RType, ())
exampleGood = (l w', ()) where
  l true = WinningCycle
  l false = LosingCycle
  w' = if true then false else true

exampleBad :: (RType, ())
exampleBad = (if w' then WinningCycle else LosingCycle, ()) where
  w' = if true then false else true
